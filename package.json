{
  "name": "@rjdellecese/elm-ts",
  "version": "0.8.1",
  "description": "A porting of TEA to TypeScript featuring fp-ts, RxJS and React",
  "files": [
    "lib",
    "es6"
  ],
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/rjdellecese/elm-ts.git"
  },
  "author": "RJ Dellecese",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rjdellecese/elm-ts/issues"
  },
  "homepage": "https://github.com/rjdellecese/elm-ts",
  "tags": [
    "typescript",
    "elm",
    "fp-ts"
  ],
  "keywords": [
    "typescript",
    "elm",
    "fp-ts"
  ],
  "scripts": {
    "check": "tsc -p .",
    "lint": "eslint --max-warnings=0 . && prettier --check .",
    "pretest": "npm run check && npm run lint",
    "test": "jest",
    "posttest": "npm run docs",
    "prebuild": "rm -rf ./lib ./es6",
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "postbuild": "ts-node scripts/rewrite-es6-paths",
    "docs": "docs-ts",
    "postdocs": "ts-node scripts/docs-index",
    "prepare": "husky install"
  },
  "dependencies": {
    "history": "^5.3.0"
  },
  "peerDependencies": {
    "fp-ts": "^2.13.1",
    "react": "^18.2.0",
    "rxjs": "^7.6.0"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chalk": "^4.1.2",
    "docs-ts": "^0.7.0",
    "eslint": "^8.29.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-import": "^2.26.0",
    "fp-ts": "^2.13.1",
    "glob": "^8.0.3",
    "husky": "^8.0.2",
    "io-ts": "^2.2.20",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "monocle-ts": "^2.3.13",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.6.0",
    "sinon": "^15.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
